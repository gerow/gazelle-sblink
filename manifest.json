{
  "manifest_version": 2,

  "name": "gazelle-sblink",
  "description": "This extension allows you to easily download files on a seedbox from a gazelle tracker site.",

  "version": "1.0",

  "permissions": [
    "https://what.cd/",
    "https://tls.passthepopcorn.me/",
    "https://animebytes.tv/",
    "http://bt.cshaus.com/"
  ],

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "sblink.js"]
    }
  ],
  "web_accessible_resources": [
    "throbber.gif",
    "check16.png",
    "stop16.png"
  ]
}
