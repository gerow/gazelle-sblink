{
  "manifest_version": 2,

  "name": "gazelle-sblink",
  "description": "This extension allows you to easily download files on a seedbox from a gazelle tracker site.",

  "version": "1.0.1",

  "permissions": [
    "http://bt.cshaus.com/transmission/rpc"
  ],

  "content_scripts": [
    {
      "matches": [
        "https://what.cd/*",
        "https://tls.passthepopcorn.me/*",
        "https://animebytes.tv/*"
      ],
      "js": ["jquery.js", "transmission-client.js", "sblink.js"]
    }
  ],
  "web_accessible_resources": [
    "throbber.gif",
    "check16.png",
    "stop16.png"
  ],
  "options_page": "options.html"
}
